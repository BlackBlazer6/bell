{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "./dist/",
    "module": "commonjs",
    "target": "es5",
    "jsx": "react",
    "jsxFactory": "m",
    "allowJs": true,
    "moduleResolution": "node",
    "lib": ["dom", "es2015"],
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "strict": true
  },
  "exclude": [
    "node_modules",
    "**/bin"
  ]
}
